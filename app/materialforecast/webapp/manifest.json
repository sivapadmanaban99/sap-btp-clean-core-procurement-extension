{
  "_version": "1.59.0",
  "sap.app": {
    "id": "materialforecast",
    "type": "application",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "Material Forecast",
    "description": "Material Forecast List Report",
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      },
      "i18n": {
        "type": "ResourceBundle",
        "uri": "i18n/i18n.properties"
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    },
    "supportedThemes": [
      "sap_hcb"
    ]
  },
  "sap.ui5": {
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.120.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "models": {
      "i18n": {
        "dataSource": "i18n",
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "materialforecast.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "type": "sap.ui.model.odata.v4.ODataModel",
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }
    }
  }
}