{
  "routes": [
    {
      "source": "^/materialforecast/(.*)$",
      "target": "/$1",
      "localDir": "../materialforecast/webapp",
      "cacheControl": "no-cache, no-store, must-revalidate"
    },
    {
      "source": "^/(.*)$",
      "target": "$1",
      "destination": "srv-api",
      "csrfProtection": true
    }
  ]
}
